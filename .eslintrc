{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": {
    "react/prop-types": [
      "error",
      {
        "ignore": ["className", "history", "children", "match", "location", "style"]
      }
    ],
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "no-console": "off"
  }
}
